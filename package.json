{
  "name": "delino",
  "version": "2.0.2",
  "description": "simple food shop with sass, flexbox and grid",
  "main": "index.js",
  "scripts": {
    "live": "http-server",
    "watch": "sass sass/main.scss:css/style.min.css --style=compressed --watch",
    "start": "concurrently \"http-server\" \"sass sass/main.scss:css/style.min.css --style=compressed --watch\"",
    "build": "sass sass/main.scss:css/style.min.css --style=compressed"
  },
  "author": "Mostafa Bodagh Abadi",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "http-server": "^14.1.1",
    "sass": "^1.87.0"
  }
}
